<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ReferenceTextView">
        <attr name="trimExpandedText" format="string|reference" />
        <attr name="trimCollapsedText" format="string|reference" />
        <attr name="trimLength" format="integer" />
        <attr name="showTrimExpandedText" format="boolean" />
        <attr name="colorClickableText" format="color|reference" />
        <attr name="referenceColor" format="color|reference" />
        <attr name="referenceContent" format="string|reference" />
        <attr name="trimLines" format="integer" />
        <attr name="trimMode">
            <enum name="trimModeLine" value="0"/>
            <enum name="trimModeLength" value="1"/>
        </attr>
    </declare-styleable>
</resources>